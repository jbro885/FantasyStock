<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fantasystock.fantasystock">

    <uses-permission android:name="android.permission.INTERNET" />

    <!--- these lines should be deleted in lieu of our custom Parse builder -->
    <meta-data
        android:name="com.parse.APPLICATION_ID"
        android:value="FantasyStockID" />
    <meta-data
        android:name="com.parse.CLIENT_KEY"
        android:value="FantasyStockKey" />
    <!--- end of delete -->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/facebook_app_id"/>

        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.DetailActivity" />
        <activity android:name=".Activities.TransactionActivity" />
        <activity android:name=".Activities.SignupActivity"></activity>
    </application>

</manifest>
